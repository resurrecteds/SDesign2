package il.ac.technion.cs.sd.app.mail;

enum RequestType {
	enum RequestType {
		SEND_MAIL {
			public String toString() {
				return "SEND_MAIL";
			}
			
			int value() {
				return 1;
			}
			
		},
		
		REQUEST_GET_NEW_MAIL {
			public String toString() {
				return "GET NEW MAIL";
			}
			
			int value() {
				return 2;
			}
		}
		
	};
}
